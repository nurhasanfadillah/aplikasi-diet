<?xml version="1.0" encoding="utf-8"?>
<full-backup-content>
    <!-- Include user data for backup -->
    <include domain="sharedpref" path="."/>
    <include domain="database" path="."/>
    
    <!-- Exclude temporary files -->
    <exclude domain="file" path="temp/"/>
    <exclude domain="file" path="cache/"/>
</full-backup-content>